<mxDefaultToolbar>
	<add as="save" action="save" icon="Scripts/mxGraph/src/editor/images/save.gif"/>
	<add as="show" action="show" icon="Scripts/mxGraph/src/editor/images/preview.gif"/>
	<add as="print" action="print" icon="Scripts/mxGraph/src/editor/images/print.gif"/>
	<add as="exportImage" action="exportImage" icon="Scripts/mxGraph/src/editor/images/image.gif"/>
	<br/><hr/>
	<add as="select" mode="select" icon="Scripts/mxGraph/src/editor/images/select.gif"/>
	<add as="pan" mode="pan" icon="Scripts/mxGraph/src/editor/images/pan.gif"/>
	<add as="connect" mode="connect" icon="Scripts/mxGraph/src/editor/images/connect.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge !== null)
			{
        editor.setEdgeTemplateName("transition");
			}
		}
	]]></add>
	<add as="message" mode="connect" icon="Scripts/mxGraph/src/editor/images/message.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge !== null)
			{
        editor.setEdgeTemplateName("message");
			}
		}
	]]></add>
	<br/><hr/>
	<add as="undo" action="undo" icon="Scripts/mxGraph/src/editor/images/undo.gif"/>
	<add as="redo" action="redo" icon="Scripts/mxGraph/src/editor/images/redo.gif"/>
	<br/><hr/>
	<add as="cut" action="cut" icon="Scripts/mxGraph/src/editor/images/cut.gif"/>
	<add as="copy" action="copy" icon="Scripts/mxGraph/src/editor/images/copy.gif"/>
	<add as="paste" action="paste" icon="Scripts/mxGraph/src/editor/images/paste.gif"/>
	<add as="delete" action="delete" icon="Scripts/mxGraph/src/editor/images/delete.gif"/>
	<br/><hr/>
	<add as="group" action="group" icon="Scripts/mxGraph/src/editor/images/group.gif"/>
	<add as="ungroup" action="ungroup" icon="Scripts/mxGraph/src/editor/images/ungroup.gif"/>
	<br/><hr/>
  <add as="Start" template="start" style="symbol;image=Scripts/mxGraph/src/editor/images/symbols/event.png" icon="Scripts/mxGraph/src/editor/images/symbols/small_event.gif"/>
  <add as="End" template="end" style="symbol;image=Scripts/mxGraph/src/editor/images/symbols/event_end.png" icon="Scripts/mxGraph/src/editor/images/symbols/small_event_end.gif"/>
  <br/><hr/>
  <add as="Split" template="gateway-split" style="symbol;image=Scripts/mxGraph/src/editor/images/symbols/fork.png" icon="Scripts/mxGraph/src/editor/images/symbols/small_fork.gif"/>
  <add as="Join" template="gateway-join" style="symbol;image=Scripts/mxGraph/src/editor/images/symbols/merge.png" icon="Scripts/mxGraph/src/editor/images/symbols/small_merge.gif"/>
  <br/><hr/>
	<!--<add as="Pool" template="pool" icon="Scripts/mxGraph/src/editor/images/swimlane.gif"/>-->
  
	<add as="Task" template="task" icon="Scripts/mxGraph/src/editor/images/symbols/task.gif"/>
  <add as="SubProcess" template="subprocess" style="symbol;image=Scripts/mxGraph/src/editor/images/symbols/subprocess.png" icon="Scripts/mxGraph/src/editor/images/symbols/small_subprocess.gif"/>
	<br/><hr/>
	<add as="fit" action="fit" icon="Scripts/mxGraph/src/editor/images/zoom.gif"/>
	<add as="zoomIn" action="zoomIn" icon="Scripts/mxGraph/src/editor/images/zoomin.gif"/>
	<add as="zoomOut" action="zoomOut" icon="Scripts/mxGraph/src/editor/images/zoomout.gif"/>
	<add as="actualSize" action="actualSize" icon="Scripts/mxGraph/src/editor/images/zoomactual.gif"/>
	<add as="zoom" action="zoom" icon="Scripts/mxGraph/src/editor/images/zoom.gif"/>
	<br/><hr/>
	<add as="outline" action="toggleOutline" icon="Scripts/mxGraph/src/editor/images/outline.gif"/>
	<add as="tasks" action="toggleTasks" icon="Scripts/mxGraph/src/editor/images/tasks.gif"/>
	<add as="help" action="toggleHelp" icon="Scripts/mxGraph/src/editor/images/help.gif"/>
	<add as="console" action="toggleConsole" icon="Scripts/mxGraph/src/editor/images/console.gif"/>
</mxDefaultToolbar>
