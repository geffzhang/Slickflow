@{
    Layout = null;
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Process Data</title>
</head>
<body>
    <!-- Modal -->
    <div class="modal-body">
        <div class="row">
            <b class="icoBtn btnNew" onclick="processlist.createProcess();" title="Create"></b>
            <b class="icoBtn btnSetting" onclick="processlist.editProcess();" title="Edit"></b>
            <b class="icoBtn btnUp" onclick="processlist.upgradeProcess();" title="Upgrade"></b>
            <b class="icoBtn btnRefresh" onclick="processlist.refreshProcess();" title="Delete"></b>
            <div id="myProcessGrid" class="ag-bootstrap" style="width:100%;height:300px;float:left;"></div>
            <img src="~/Content/images/ajax-loader.gif" id="loading-indicator" style="position: fixed; left: 40%; top: 40%; display: none;" />
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary lang" data-dismiss="modal" as="sure" onclick="processlist.sure();"></button>
        <button class="btn lang" data-dismiss="modal" aria-hidden="true" as="cancel"></button>
        <button class="btn btn-danger lang" as="delete" onclick="processlist.deleteProcess();"></button>
    </div>
   
    <script type="text/javascript">
        $(function () {
            processlist.init();
            processlist.getProcessList();
        });
    </script>
</body>
</html>